!!! Strict
%html{:xmlns=>"http://www.w3.org/1999/xhtml","xml:lang"=>:en,:lang=>:en}
  %head
    = stylesheet_link_tag 'generic', :media => 'all'
    = stylesheet_link_tag 'vbo', :media => 'all'
    = javascript_include_tag :defaults

  %body{:id=>"#{h(controller.controller_name)}_#{h(controller.action_name)}", :class=>(SANDBOX ? :testing : nil)}
    #outer_wrapper_admin
      #nav
        = render :partial => 'layouts/login_banner'
        %ul#store_nav= nav_tabs 'boxoffice_nav', [['Walkup Sales', :box_office, :walkup], ['Printable Door List', :report, :boxoffice_report, '', {:showdate_id => @gShowdate}]]
        %br.clear
      #inner_wrapper_admin
        - unless flash[:notice].blank?
          %p.feedback.clear= flash[:notice]
        = render :partial => 'show_stats'
        = yield

    = render :partial => 'layouts/footer'





